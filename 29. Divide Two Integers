class Solution {
public:
   	int divide(int dividend, int divisor) {
		bool minus = !(dividend<0 == divisor<0);

		long long dividend2 = dividend;
		long long divisor2 = divisor;
		long long result = dividend2 / divisor2;

		if (result >= 2147483648) return 2147483647;

		return dividend / divisor;
    }
};