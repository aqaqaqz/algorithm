/*
// Definition for a Node.
class Node {
public:
    int val;
    vector<Node*> children;

    Node() {}

    Node(int _val, vector<Node*> _children) {
        val = _val;
        children = _children;
    }
};
*/
class Solution {
    vector<int> result;
public:
    vector<int> preorder(Node* root) {
        if(root==NULL) return result;
        
        result.push_back(root->val);
        for(Node* n : root->children)
            preorder(n);
        
        return result;
    }
};