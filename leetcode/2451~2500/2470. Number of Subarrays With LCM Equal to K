class Solution {
public:
    int getGcd(int a, int b){
        int c;

        while (b != 0){
            c = a % b;
            a = b;
            b = c;
        }

        return a;
    }

    int getLcm(int a, int b){
        return a * b / gcd(a, b);
    }

    int subarrayLCM(vector<int>& nums, int k) {
        int result = 0;

        for(int i=0;i<nums.size();i++){
            int lcm = nums[i];
            for(int j=i;j<nums.size();j++){
                lcm = getLcm(lcm, nums[j]);
                if(lcm == k) result++;
                if(lcm > k) break;
            }
        }

        return result;
    }
};