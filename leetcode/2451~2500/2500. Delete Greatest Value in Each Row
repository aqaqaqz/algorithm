class Solution {
public:
    int deleteGreatestValue(vector<vector<int>>& grid) {
        int result = 0;

        for(vector<int>& v : grid)
            sort(v.begin(), v.end());

        for(int w=0;w<grid[0].size();w++){
            int temp = 0;

            for(int h=0;h<grid.size();h++)
                temp = max(temp, grid[h][w]);

            result += temp;
        }

        return result;
    }
};