class Solution {
public:
    int longestPalindrome(string s) {
        unordered_map<char, int> m;
        for(char c : s) m[c]++;
        
        bool odd = false;
        int result = 0;
        for(auto iter = m.begin();iter!=m.end();iter++){
            if(iter->second%2 == 1){
                odd = true;
                result += (iter->second-1);
            }else result += iter->second;
        }
        return result+(odd?1:0);
    }
};